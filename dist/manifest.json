{
  "manifest_version": 2,
  "name": "NoHorny",
  "version": "1.0",
  "description": "Removes NSFW images with some flare.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "sprites/*",
    "sounds/*"
  ]
}
